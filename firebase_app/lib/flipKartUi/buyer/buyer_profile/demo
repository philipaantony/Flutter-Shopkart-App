// ignore: must_be_immutable
class BuyerProfile extends StatefulWidget {
  String? name;
  String? email;
  String? currentuserid;
  String? moblienumber;
  String? pin;
  String? address;
  String? state;
  String? imageurl;

  BuyerProfile(
      {this.name,
      this.imageurl,
      this.email,
      this.moblienumber,
      this.address,
      this.pin,
      this.state,
      this.currentuserid,
      Key? key})
      : super(key: key);

  @override
  State<BuyerProfile> createState() => _BuyerProfileState();
}

